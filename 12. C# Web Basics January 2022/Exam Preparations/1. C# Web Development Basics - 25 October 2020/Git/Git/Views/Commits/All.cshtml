<div class="container">
    <div class="section">
        <div class="row">
            <div class="col-md-8 offset-md-2">
                <div class="page-header text-center">
                    <h2 id="tables">My Commits</h2>
                </div>

                <div class="component">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th scope="col">Repository</th>
                                <th scope="col">Description</th>
                                <th scope="col">Created On</th>
                                <th scope="col">Delete</th>

                            </tr>
                        </thead>
                        <tbody>
                            {{foreach}}
                            {
                                <tr class="table-light">
                                    <th scope="row">{{Repository}}</th>
                                    <td>{{Description}}</td>
                                    <td>{{CreatedOn}}</td>
                                    <td>
                                        <a class="btn btn-block btn-primary text-white mb-3" href="/Commits/Delete?id={{Id}}">Delete</a>
                                    </td>
                                </tr>
                            }                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>